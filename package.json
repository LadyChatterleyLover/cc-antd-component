{
  "name": "cc-antd-component",
  "version": "1.0.0",
  "author": {
    "name": "cc"
  },
  "description": "二次封装antd组件库",
  "keywords": [
    "react",
    "umi",
    "antd",
    "component",
    "react-components",
    "antd-components",
    "组件库",
    "组件",
    "二次封装",
    "二次封装组件"
  ],
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@types/nprogress": "^0.2.0",
    "ahooks": "^3.0.0",
    "axios": "^0.24.0",
    "nprogress": "^0.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.20",
    "wangeditor": "^4.7.10"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
